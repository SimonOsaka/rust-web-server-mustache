[workspace]
members = [
    "{{#warp_api}}{{member_name}}{{/warp_api}}", 
    "{{#repository}}{{member_name}}{{/repository}}", 
    "{{#domain}}{{member_name}}{{/domain}}", 
    "{{#types}}{{member_name}}{{/types}}", 
    "{{#repository}}{{member_name}}{{/repository}}",
    "{{#search}}{{member_name}}{{/search}}",
    "{{#axum_api}}{{member_name}}{{/axum_api}}",
    "{{#server}}{{member_name}}{{/server}}",
    "{{#auth}}{{member_name}}{{/auth}}",
    "{{#logger}}{{member_name}}{{/logger}}",
    "{{#extra}}{{member_name}}{{/extra}}",
    "{{#i18n}}{{member_name}}{{/i18n}}",
    "{{#util}}{{member_name}}{{/util}}"
    ]

[profile.dev]
lto = false
opt-level = 0
overflow-checks = true
incremental = true
split-debuginfo = "unpacked"

[profile.release]
lto = "thin"
opt-level = 'z'
overflow-checks = false
incremental = false
panic = "abort"

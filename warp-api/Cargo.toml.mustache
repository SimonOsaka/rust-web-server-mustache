[package]
name = "{{#warp_api}}{{package_name}}{{/warp_api}}"
version = "0.1.0"
edition = "2021"

# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html
[features]
postgres = []
mysql = []

[dependencies]
{{#types}}{{package_name}}{{/types}} = { path = "../{{#types}}{{member_name}}{{/types}}", features = ["{{features_name}}"] }
{{#domain}}{{package_name}}{{/domain}} = { package = "{{#domain}}{{package_name}}{{/domain}}", path = "../{{#domain}}{{member_name}}{{/domain}}", features = ["{{features_name}}"] }
{{#auth}}{{package_name}}{{/auth}} = { path = "../{{#auth}}{{member_name}}{{/auth}}" }
warp = "0.3"
chrono = "0.4.19"
flexi_logger = "0.19.3"
log = "0.4"
serde = "1.0.125"
serde_derive = "1.0.125"
async-trait = "0.1"

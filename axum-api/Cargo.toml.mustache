[package]
name = "{{#axum_api}}{{package_name}}{{/axum_api}}"
version = "0.1.0"
edition = "2021"

# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html

[dependencies]
{{#types}}{{package_name}}{{/types}} = { path = "../{{#types}}{{member_name}}{{/types}}" }
{{#domain}}{{package_name}}{{/domain}} = { path = "../{{#domain}}{{member_name}}{{/domain}}" }
{{#auth}}{{package_name}}{{/auth}} = { path = "../{{#auth}}{{member_name}}{{/auth}}" }
{{#util}}{{package_name}}{{/util}} = { path = "../{{#util}}{{member_name}}{{/util}}", features = ["excel"] }
axum = { version = "0.4" }
tokio = { version = "1.0", features = ["full"] }
tower = { version = "0.4", features = ["util", "filter", "timeout"] }
tower-http = { version = "0.2", features = ["trace", "add-extension"] }
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
jsonwebtoken = "8"
headers = "0.3"
async-trait = "0.1"
hyper = { version = "0.14", features = ["full"] }
validator = { version = "0.14.0", features = ["derive"] }
http-body = "0.4"
thiserror = "1.0"
tracing = { path = "../crates/tracing", version = "0.2" }
